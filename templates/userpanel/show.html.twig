{% extends 'frontbase.html.twig' %}

{% block title %}Shopcart{% endblock %}
{% block sidebar %}{{ include('home/_usersidebar.html.twig') }}{% endblock %}
{% block content %}


    <ul class="breadcrumb">
            <li><a href="{{ path('home') }}">Home</a></li>
            <li><a href="">Shopcart Items</a></li>
            <li class="active">Preview</li>
    </ul>

    <div class="span9" style="background: white">
        <div class="row-fluid">
            <h1>Ãœye Profil Bilgileri</h1>

            <table class="table">
                <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ app.user.id }}</td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td>{{ app.user.email }}</td>
                </tr>
                <tr>
                    <th>Roles</th>
                    <td>
                        {{ app.user.roles|join }}
                    </td>
                </tr>
                <tr>
                    <th>Password</th>
                    <td>{{ app.user.password }}</td>
                </tr>
                <tr>
                    <th>Name</th>
                    <td>{{ app.user.name }}</td>
                </tr>
                <tr>
                    <th>Status</th>
                    <td>{{ app.user.status }}</td>
                </tr>
                </tbody>
            </table>

        </div>
    </div>


{% endblock %}
